netbeans/ctp_map/nbproject/Makefile-impl.mk:	"${MAKE}" -f nbproject/Makefile-${CONF}.mk SUBPROJECTS=${SUBPROJECTS} .build-tests-conf
netbeans/ctp_map/nbproject/Makefile-impl.mk:.test-impl: .build-tests-impl .test-pre
